[xdebug]
xdebug.remote_enable=1
xdebug.remote_connect_back=<%= scope.lookupvar('env::xdebug_remote_connect_back')%>
xdebug.remote_autostart=<%= scope.lookupvar('env::xdebug_remote_autostart')%>
xdebug.remote_port=<%= scope.lookupvar('env::xdebug_remote_port')%>
xdebug.profiler_enable_trigger=<%= scope.lookupvar('env::xdebug_profiler_enable_trigger')%>
xdebug.profiler_output_name=<%= scope.lookupvar('env::xdebug_profiler_output_name')%>
xdebug.profiler_output_dir=<%= scope.lookupvar('env::xdebug_profiler_output_dir')%>
xdebug.idekey=PHPSTORM